msgid "Linearity"
msgstr ""

#: ../../stages/filters.covariancefeatures.rst:24
msgid "Omnivariance"
msgstr ""

#: ../../stages/filters.covariancefeatures.rst:25
msgid "Planarity"
msgstr ""

#: ../../stages/filters.covariancefeatures.rst:26
